---
hostinfo:
  externalid: 10001
  pop: location
  srvtype: srvtype
  status: status
sapi:
  #sapi_do_api: true
  sapi_do_api: false

  sapi_endpoint: "https://sapi.manowar.example.com/sapi/puthostjson/"

  # By defaults adds the token as a header
  sapi_token: InsertToken
  sapi_username: ChangeMe

  # With these you can authorize and send the output to any endpoint
  # That will let you configure authorization via headers or query args
  custom_header:
    "Content-Type": "application/json"
  custom_query_args:
    "sample": "true"
stingcell:
  default_collection_timeout: 60
  local_collections: false
  local_collections_location: /etc/saltcell/collections.d/
  collection_config_file: etc/saltcell/collector.yaml.sample
ipintel:
  dointel: true
version: 3
salt:
  minion_file: etc/saltcell/minion
